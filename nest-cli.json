{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/backend-tech-test/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/backend-tech-test/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/backend-tech-test",
  "projects": {
    "backend-tech-test": {
      "type": "application",
      "root": "apps/backend-tech-test",
      "entryFile": "main",
      "sourceRoot": "apps/backend-tech-test/src",
      "compilerOptions": {
        "tsConfigPath": "apps/backend-tech-test/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "task": {
      "type": "application",
      "root": "apps/task",
      "entryFile": "main",
      "sourceRoot": "apps/task/src",
      "compilerOptions": {
        "tsConfigPath": "apps/task/tsconfig.app.json"
      }
    }
  }
}